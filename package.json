{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "babel-node ./bin/www node",
    "both": "concurrently \"npm start\" \"cd client && npm start\"",
    "dev": "nodemon --exec babel-node ./bin/www",
    "test": "./node_modules/.bin/mocha --require @babel/register --exit --timeout 30000",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build && npm install babel-cli -S"
  },
  "engines": {
    "node": "10.16.3"
  },
  "proxy": "http://localhost:3001",
  "dependencies": {
    "aws-sdk": "^2.814.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "express": "~4.16.1",
    "express-validator": "^6.2.0",
    "http": "0.0.0",
    "http-errors": "~1.6.3",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "mongodb": "^3.3.2",
    "mongoose": "^5.7.3",
    "mongoose-beautiful-unique-validation": "^7.1.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
